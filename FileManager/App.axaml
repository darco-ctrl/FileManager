<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="FIleManager.App"
             xmlns:local="using:FIleManager"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>


	<Application.Styles>
    		<FluentTheme />
		<StyleInclude Source="avares://FileManagerAvalonia/Styles/DefualtAppStyle.axaml"></StyleInclude>
    </Application.Styles>

	<Application.Resources>
		<SolidColorBrush x:Key="PrimaryBackground">#FF191919</SolidColorBrush>
	</Application.Resources>
</Application>